  "version": "0.2.0",
  "configurations": [
    {
      "name": "(Linux) Build & Run",
      "request": "launch",
      "type": "cppdbg",
      "stopAtEntry": false,
      "preLaunchTask": "Build with CMake",
      "cwd": "${workspaceRoot}/build/",
      "program": "${workspaceRoot}/build/Ellipsoid"
    },
    {
      "name": "(Windows) Build & Run",
      "request": "launch",
      "type": "cppvsdbg",
      "stopAtEntry": false,
      "preLaunchTask": "Build with CMake",
      "cwd": "${workspaceRoot}/build/",
      "program": "${workspaceRoot}/build/Debug/Ellipsoid.exe"
    }
  ]
}
